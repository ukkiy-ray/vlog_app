<div class="post-form-wrapper">
  <div class="form-container">
    <%= form_with model: @post, local: true do |f| %>
      <p class="post-title-form">title</p><%= f.text_field :title, class: "title-input" %> <br>
      <p class="form-video">video</p><%= f.file_field :video, class: "video-file" %> <br>
      <div class="submit-post">
        <%= f.submit "Submit", class: "submit-btn" %>
      </div>
    <% end %>
  </div>
</div>